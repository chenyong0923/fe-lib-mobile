{
  "name": "uniubi-lib-mobile-demo",
  "version": "0.23.0",
  "private": true,
  "description": "移动端组件库 demo 项目",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "less"
  },
  "scripts": {
    "dev:h5": "npm run build:h5 -- --watch",
    "build:h5": "cross-env CUSTOM_ENV=dev taro build --type h5",
    "build:h5:production": "cross-env CUSTOM_ENV=production taro build --type h5",
    "dev:weapp": "npm run build:weapp -- --watch",
    "build:weapp": "cross-env CUSTOM_ENV=dev taro build --type weapp",
    "build:weapp:production": "cross-env CUSTOM_ENV=production taro build --type weapp",
    "lint": "eslint 'src/**/*.{js,jsx,tsx,ts}' --quiet",
    "lint:fix": "eslint 'src/**/*.{js,jsx,tsx,ts}' --fix --quiet",
    "prettier": "prettier --write '**/*.{less,css,md,json}'"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/components": "3.6.8",
    "@tarojs/helper": "3.6.8",
    "@tarojs/plugin-framework-react": "3.6.8",
    "@tarojs/plugin-platform-h5": "3.6.8",
    "@tarojs/react": "3.6.8",
    "@tarojs/runtime": "3.6.8",
    "@tarojs/taro": "3.6.8",
    "@tarojs/webpack5-runner": "3.6.8",
    "@uniubi/icons-taro": "^3.50.0",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uniubi-lib-mobile": "^0.23.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@types/qs": "^6.9.7",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/webpack-env": "^1.13.6",
    "babel-plugin-import": "^1.13.6",
    "babel-plugin-transform-jsx-stylesheet": "^1.0.0",
    "babel-preset-taro": "3.6.8",
    "cross-env": "^7.0.2",
    "postcss": "^8.4.23",
    "react-refresh": "^0.14.0",
    "rimraf": "^3.0.2",
    "webpack": "^5.75.0"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "xuxiu <zhangming@uni-ubi.com>",
  "repository": {
    "type": "git",
    "url": "git@gitlab.uniubi.com:fe-team/common/uniubi-lib-mobile.git"
  }
}
